    "autoprefixer": "^10.4.20",
    "concurrently": "^7.0.0",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.0",
